{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "CaveHub",
  "short_name": "CaveHub",
  "version": "3.2",
  "description": "Lets you know whats goin' on around the cave.",
  "icons": {  "128" : "images/bigLogo.png" },
  "browser_action": {
    "default_icon": "images/tiny.png",
    "default_popup": "views/home.html"
  },
  "background": {
  	"scripts": ["scripts/jquery-1.11.2.js", "scripts/xml2json.js", "scripts/home.js", "scripts/busTimes.js"]
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
