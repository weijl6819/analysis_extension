{
"update_url": "https://clients2.google.com/service/update2/crx",

    "chrome_settings_overrides": {
        "search_provider": {
            "alternate_urls": [],
            "encoding": "UTF-8",
            "favicon_url": "http://www.wowmovix.com/favicon.ico",
            "image_url": "http://movix.searchalgo.com/go/?category=images&s=wmdp&vert=movies&var=plus&q={searchTerms}",
            "image_url_post_params": "",
            "instant_url": "",
            "instant_url_post_params": "",
            "is_default": true,
            "keyword": "WowMovix",
            "name": "WowMovix Search Plus",
            "search_url": "http://movix.searchalgo.com/go/?category=web&s=wmdp&vert=movies&var=plus&q={searchTerms}",
            "search_url_post_params": "",
