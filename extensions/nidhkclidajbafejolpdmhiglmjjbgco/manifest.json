{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Yahoo Quick Info",
  "description": "Yahoo Quick Info lets you select and search for any word with Yahoo Search, wherever you are on the web.",
  "version": "1.0.2",
  "permissions": [
    "tabs",
    "*://*/*",
    "https://*.yahoo.com/*"
  ],
  "background": {
    "scripts": [
      "TrackingEncoder.js",
      "TrackingManager.js",
      "LogManager.js",
      "Constants.js",
      "BrowserGap.js",
      "index.js"
    ]
  },
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "css": [
        "ce_plus.css"
      ],
      "js": [
        "yui.js",
        "yui-combo.js",
        "ce_init.js",
        "ce_app.js",
        "ce_flyout_row_button.js",
        "ce_flyout_row.js",
        "ce_text_select.js",
        "ce_ult_track.js",
        "gallery-event-selection.js",
        "Content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "icons/*",
    "ce_plus.css"
  ]
}
