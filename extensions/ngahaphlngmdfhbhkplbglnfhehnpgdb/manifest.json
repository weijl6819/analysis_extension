{
    "background": {
        "scripts": ["lib/jquery/jquery.min.js", "bg.js"]
    },
    "browser_action": {
        "default_icon": "images/enabled.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
	    "matches": ["http://*/*", "https://*/*"],
        "js": ["getUrl.js"],
        "run_at": "document_start"
    }, {  
        "all_frames": true,
        "matches": ["http://*/*", "https://*/*"],
        "js": ["lib/jquery/jquery.min.js", "css.js"],
        "run_at": "document_start"
    }],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self'",
	"description": "__MSG_ext_description__",
	"default_locale": "en",
	"icons": {
		"32": "images/enabled.png",
		"128": "images/icon.png"
	},
	"manifest_version": 2,
	"homepage_url": "http://adsblocker.ru",
	"name": "__MSG_ext_name__",
	"short_name": "__MSG_ext_name_short__",
	"permissions": ["tabs", "webRequest", "webRequestBlocking", "webNavigation", "management", "\u003Call_urls>", "storage"],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "6.0.1"
}