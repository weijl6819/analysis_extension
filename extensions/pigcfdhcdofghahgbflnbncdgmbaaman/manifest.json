{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Kindle Books",
  "description": "Free Kindle Books and Kindle Book Deals are directly in your browser",
  "version": "1.0",

  "permissions": [
    "http://kebooks.com/"
  ],
  "browser_action": {
  	"default_icon": "icon_48.png"
  },
  "icons": {
    	"48": "icon_48.png",
    	"128": "icon_128.png"
  },
  "background": {
  	"scripts": [ "js/background.js" ]
  }
}