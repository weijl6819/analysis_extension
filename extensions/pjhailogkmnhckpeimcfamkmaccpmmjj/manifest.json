{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "65daigou EZcart",
	"version": "1.4.1",  
  	"description": "Instantly add items to 65daigou's shopping cart while browsing on Taobao/Tmall website.",  
	"icons" : {"16":"images/logo16.png","32":"images/logo32.png","96":"images/logo96.png","128":"images/logo128.png"},
	"content_scripts": [
		{
			"matches": [
				"*://world.taobao.com/item*",
				"*://world.tmall.com/item*",
				"*://detail.tmall.com/item.htm*",
				"*://item.taobao.com/item.htm*",
				"*://www.amazon.com/*",
				"*://www.carters.com/*",
				"*://www.oshkosh.com/*",
				"*://www.ralphlauren.com/*",
				"*://www.abercrombiekids.com/*",
				"*://www.abercrombie.com/*",
				"*://www.forever21.com/*",
				"*://www.drugstore.com/*",
				"*://*.gap.com/*",
				"*://store.nike.com/*",
				"*://www.sephora.com/*",
				"*://www.jackspade.com/*",
				"*://www.katespade.com/*",
				"*://www.coach.com/*",
				"*://www.ebay.com/*",
				"*://www.target.com/*",
				"*://www.disneystore.com/*",
				"*://www.bathandbodyworks.com/*",
				"*://colourpop.com/*",
				"*://www.elfcosmetics.com/*",
				"*://www.crocs.com/*",
				"*://www.6pm.com/*",
				"*://www.gnc.com/*",
				"*://www.timberland.com/*",
				"*://www.aldoshoes.com/*",
				"*://www.bestbuy.com/*",
				"*://*.jd.com/*"
			],
			"js": ["jslib/jquery-2.1.4.min.js","jslib/laytpl.js","jslib/lodash-3.10.1.min.js","script.js"],
			"css": ["inject.css"]
		}
	],
	"externally_connectable":{
		"matches":["*://*.taobao.com/*","*://*.tmall.com/*"]
	},
	"web_accessible_resources":[
		"images/*.png",
		"jslib/*.js"
	],
	"manifest_version": 2,
	"page_action": {
		"default_icon" : "images/logo96.png",
		"default_title" : "Ezcart"
	},
	"permissions": [
		"*://detail.tmall.com/*",
		"storage",
		"tabs"
    ],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}