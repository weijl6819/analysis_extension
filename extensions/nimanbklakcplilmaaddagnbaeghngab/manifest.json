{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "EnlightKS Lightweight Candidate",
  "version": "1.0.0.5",
  "manifest_version": 2,
  "description": "EnlightKS Lightweight Candidate extension for Chrome",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "nativeMessaging"
  ],
  "externally_connectable": {
  "matches": ["*://*.enlightks.com/*" , "*://*.enlight.net/*"]
  },
  "icons": { "16": "logo16.png",
	     "32": "logo32.png",
	     "48": "logo48.png",
	     "128": "logo128.png"     
  }
}
