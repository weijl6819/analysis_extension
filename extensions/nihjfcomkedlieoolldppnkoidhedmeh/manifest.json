{
"update_url":"http://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Distilled Notifications",
    "description": "Receive Distilled notifications within your browser",
    "version": "1.0",
    "icons": {
        "128": "distilled-128.png"
    },
    "browser_action": {
        "default_title": "",
        "default_icon": "distilled-small.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "jquery.min.js",
            "background.js"
        ]
    },
    "permissions": [

        "http://www.distilled.net/"

    ]
}