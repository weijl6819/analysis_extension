{
"update_url": "https://clients2.google.com/service/update2/crx",

   "browser_action": {
      "default_icon": {
         "128": "images/fsi_scout_icon128.png",
         "48": "images/fsi_scout_icon48.png",
         "32": "images/fsi_scout_icon32.png",
         "16": "images/fsi_scout_icon16.png"
      },
      "default_popup": "popup.html",
      "default_title": "Farsight DNSDB Scout™"
   },
   "content_security_policy": "script-src 'self' https://api.dnsdb.info; object-src 'self'",
   "description": "Chrome Extension GUI for Farsight Security DNSDB™.",
   "icons": {
      "128": "images/fsi_scout_icon128.png",
      "48": "images/fsi_scout_icon48.png",
      "32": "images/fsi_scout_icon32.png",
      "16": "images/fsi_scout_icon16.png"
   },
   "manifest_version": 2,
   "name": "Farsight DNSDB Scout™",
   "options_page": "options.html",
   "permissions": [ "storage", "unlimitedStorage" ],
   "version": "1.0.5"
}
