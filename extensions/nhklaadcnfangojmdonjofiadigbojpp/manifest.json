{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Smat Social",
  "version": "0.93",
  "description": "Grow Fast with Smat-Social !!",
  "permissions": ["cookies", "tabs", "http://*/*", "https://*/*", "notifications"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Smat Social",
    "default_icon": "icon.png"
  },
  "content_scripts": [{
    "js": [ "random.js" ],
    "matches": [ "http://*/*", "https://*/*", "file://*/*" ]
  } ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}
