{
  "browser_action": {
    "default_icon": "/images/icons/gett_19.png",
    "default_popup": "index.html",
    "default_title": "Ge.tt"
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "description": "Easy file sharing with Ge.tt",
  "background": {"scripts": [
    "scripts/debug.js",
    "scripts/browser_modules.js",
    "scripts/sync/extensions.js",
    "scripts/sync/background.js",
    "scripts/background.js"
  ]},
  "name": "Ge.tt",
  "permissions": [
    "tabs",
    "contextMenus",
    "clipboardWrite",
    "http://*/"
  ],
  "options_page": "options.html",
  "icons": {
    "48": "/images/icons/gett_48.png",
    "128": "/images/icons/gett_128.png",
    "16": "/images/icons/gett_16.png"
  },
  "manifest_version": 2,
  "version": "1.1.5"
}