{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Maxwidth",
  "version": "1.3.4",
  "manifest_version": 2,
  "description": "Prevents Tumblr from stretching lower width images to 500px.",
  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
             "128": "icon_128.png" },
  "content_scripts": [
    {
      "matches": ["http://www.tumblr.com/*", "https://www.tumblr.com/*"],
      "css": ["main.css"]
    }
  ]
}