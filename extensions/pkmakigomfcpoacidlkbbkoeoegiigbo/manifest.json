{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Unblur StudyBlue",
  "short_name": "UnBlur",
  "description": "This extension unblurs the study cards from StudyBlue",
  "version": "1.0",

  "content_scripts": [
	{
		"matches": ["http://www.studyblue.com/*","https://www.studyblue.com/*"],
		"css": ["undo.css"]
	}
  ],
  "icons": {
	  "16": "icon16.png",
	  "48": "icon48.png",
	  "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
