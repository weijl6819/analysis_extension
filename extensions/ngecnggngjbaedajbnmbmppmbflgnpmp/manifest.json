{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "First Line",
    "description": "Displays a random first sentence of a book.",
    "version": "0.2",
    "manifest_version": 2,
    "permissions" : ["management"],
    "chrome_url_overrides" : {
        "newtab": "newtab.html"
