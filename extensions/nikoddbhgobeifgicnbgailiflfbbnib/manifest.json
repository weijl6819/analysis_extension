{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

  "name": "Mlg Rain",
  "description": "Make it rain the Internet's most mlg things ever",
  "version": "1.2",
  "icons": { "19": "icon19x19.png",
           "48": "icon48x48.png",
          "128": "icon128x128.png" },

  "browser_action": {
   "default_icon": "icon19x19.png"
  },

  "permissions": [
    "tabs", "<all_urls>",
     "activeTab","<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icon19x19.png",
    "default_popup": "popup.html"
  },
   "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["style.css"],
        "js": ["jquery.js","js-ui.js"]
      }
    ],

  "background": {
    "scripts": ["jquery.js","js-ui.js","background.js"]
  }
}
