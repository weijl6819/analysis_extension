    <!-- LOGIN EMAIL -->
    <div id="login-mail" {{#if data.errorLogin }} class="showing" {{/if}}>

        <div class="content">

            <div class="header">
                <h2 class="text-center">Login with your e-mail</h2>
            </div>

            <form id="login-form"  action="https://www.awwwards.com/api/login" method="post" novalidate>
                <div class="form-field form-field-id-username">
                    <div class="form-error-display" ></div>
                    <label for="username" class="field-label">Email or Username</label>
                    <input name="_username" id="username" type="text" class="form-field" />
                </div>
                <div class="form-field form-field-id-pass">
                <div class="form-error-display" ></div>
                    <label for="password" class="field-label">Password</label>
                    <input name="_password" id="password" class="form-field" type="password" />
                </div>
                {{#if data.errorLogin }}
                    <div class="form-field login-error-msg" >Incorrect Username/Password, please try again.</div>
                {{/if}}
                <div class="form-field">
                    <button type="button" class="login-form-submit-btn form-field" ><span class="bt-content">LOGIN NOW!</span><div class="updating-spinner"></div></button>
                    <div class="cancel-login-mail-wrapper" >
                        <a class="cancel-login-mail form-cancel-btn">Cancel</a>
                    </div>
                </div>

                <div class="text-center pass-forgot">
                    <a href="https://www.awwwards.com/forgot-password" target="_blank" class=""><strong>Forgot your password?</strong></a>
                </div>
            </form>

            <div class="footer">
                <h4 class="text-center">Not a member yet? <a href="https://www.awwwards.com/register/" target="_blank" class="register" >Register now</a></h4>
            </div>

        </div>
    </div>

    <div id="error-screen" class="animated" >
        <div class="error-title" >Oops<br/>There are<br/>problems with<br/>connection</div>
        <div class="error-description" >Please try to log again</div>
        <button type="submit" class="form-field aw-close-error" ><span class="bt-content">Try Again</span></button>
    </div>

</section>
